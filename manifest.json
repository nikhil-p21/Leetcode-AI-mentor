{
    "manifest_version": 3,
    "name": "LeetCode AI Mentor",
    "version": "0.1.0",
    "description": "An AI-powered mentor to help you solve LeetCode problems.",
    "icons": {
      "16": "icons/icon16.ico",
      "48": "icons/icon_48.ico",
      "128": "icons/icon_128.ico"

    },
    "content_scripts": [
      {
        "matches": ["https://leetcode.com/problems/*"],
        "css": ["style.css"],
        "js": ["content.js"]
      }
    ]
  }